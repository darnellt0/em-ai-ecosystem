# Elevated Movements AI Ecosystem - Environment Configuration
# Copy this file to .env and fill in your actual values

# Core Settings
NODE_ENV=production
PORT=3000
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql://elevated_movements:changeme@database:5432/em_ecosystem
POSTGRES_PASSWORD=changeme
POSTGRES_USER=elevated_movements
POSTGRES_DB=em_ecosystem

# Cache
REDIS_URL=redis://redis:6379

# API Keys (Replace with actual keys)
OPENAI_API_KEY=sk-your-openai-key-here
CLAUDE_API_KEY=sk-your-claude-key-here
ELEVENLABS_API_KEY=your-elevenlabs-key-here

# Google Integration
GOOGLE_APPLICATION_CREDENTIALS=/app/config/google-credentials.json

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=465
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Founder Configuration
FOUNDER_DARNELL_CALENDAR_ID=your-calendar-id@gmail.com
FOUNDER_DARNELL_EMAIL=darnell@elevatedmovements.com
FOUNDER_SHRIA_CALENDAR_ID=your-calendar-id@gmail.com
FOUNDER_SHRIA_EMAIL=shria@elevatedmovements.com

# System Behavior
DEEP_WORK_MIN_DURATION_MIN=90
FIELD_TRIP_TUESDAY_ENABLED=true
MAX_BACK_TO_BACK_MEETINGS=3

# Cost Limits
MONTHLY_COST_LIMIT=5000
WARNING_COST_THRESHOLD=4000

# n8n Configuration
N8N_PASSWORD=changeme
N8N_BASIC_AUTH_ACTIVE=true
