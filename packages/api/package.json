    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^164.1.0",
    "ioredis": "^5.4.1",
    "nodemailer": "^7.0.10",
    "openai": "^6.7.0",
    "pg": "^8.16.3",
