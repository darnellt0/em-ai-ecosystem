{
  "name": "em-ai-ecosystem",
  "version": "1.0.0",
  "description": "Elevated Movements AI Executive Assistant Ecosystem - Dual-Founder Edition",
  "private": true,
  "engines": {
    "node": ">=20.19.4",
    "npm": ">=10"
  },
  "packageManager": "npm@10.7.0",
  "workspaces": [
    "packages/*",
    "packages/agents/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "dev": "lerna run dev --parallel",
    "test": "lerna run test",
    "test:coverage": "lerna run test:coverage",
    "lint": "lerna run lint",
    "format": "prettier --write \"packages/*/src/**/*.ts\" \"packages/*/tests/**/*.ts\"",
    "clean": "lerna run clean && rm -rf node_modules",
    "setup": "bash scripts/setup.sh",
    "health-check": "bash scripts/health-check.sh",
    "check-agent-integrity": "echo 'Agent integrity check requires manual execution: cd to repo root and run: npx tsx --tsconfig tsconfig.json scripts/check-agent-integrity.ts' && exit 1",
    "test:calendar": "npx tsx scripts/test-google-calendar.ts",
    "backup": "bash scripts/backup.sh",
    "kill-switch": "bash scripts/kill-switch.sh",
    "docker:up": "docker-compose -f infrastructure/docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f infrastructure/docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f infrastructure/docker/docker-compose.yml logs -f",
    "mobile:start": "cd packages/mobile && npm start",
    "mobile:ios": "cd packages/mobile && npm run ios",
    "mobile:android": "cd packages/mobile && npm run android",
    "mobile:build:android": "cd packages/mobile && npm run build:android",
    "mobile:build:ios": "cd packages/mobile && npm run build:ios"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lerna": "^9.0.3",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}
